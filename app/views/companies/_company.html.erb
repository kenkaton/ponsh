<div id="<%= dom_id company %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1"><%= t('.name') %>:</strong>
    <%= company.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.kana') %>:</strong>
    <%= company.kana %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.name_en') %>:</strong>
    <%= company.name_en %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.prefecture_code') %>:</strong>
    <%= company.prefecture_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.city') %>:</strong>
    <%= company.city %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.address') %>:</strong>
    <%= company.address %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.tel') %>:</strong>
    <%= company.tel %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.fax') %>:</strong>
    <%= company.fax %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.website') %>:</strong>
    <%= company.website %>
  </div>
  <div>
    <strong class="block font-medium mb-1"><%= t('.detail') %>:</strong>
    <%= company.detail %>
  </div>
</div>
